{
  "kind": "collectionType",
  "collectionName": "charts",
  "info": {
    "singularName": "chart",
    "pluralName": "charts",
    "displayName": "Chart"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "imgalbum": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "genres": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::genre.genre",
      "mappedBy": "chart"
    },
    "artists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist",
      "mappedBy": "chart"
    }
  }
}
